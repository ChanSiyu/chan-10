FoamFile
{
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 494.88482;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    "(back|front|up|down)"
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    body
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}

// ************************************************************************* //
